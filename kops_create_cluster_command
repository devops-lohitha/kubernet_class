kops create cluster --name kajal.tk --yes --state=s3://kubernetesbucket455555 --zones=us-east-1a,us-east-1b,us-east-1c --node-count=2 --node-size=t3.medium --master-count=1 --master-size=t3.medium


 kops update cluster --name kajal.tk --yes --admin --state s3://kubernetesbucket455555
 
 kops validate cluster --name kajal.tk --state s3://kubernetesbucket455555
